    "@vercel/ncc": "0.27.0",
    "eslint": "7.25.0",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-jest": "24.3.5",
    "graceful-fs": "4.2.6",
    "jest": "26.6.3",
