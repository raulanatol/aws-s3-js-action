{
  "name": "aws-s3-js-action",
  "version": "1.0.0",
  "private": true,
  "description": "Synchronize S3 bucket files",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "license": "MIT",
  "author": "raulanatol",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf dist lib",
    "lint": "eslint --max-warnings 0 . --ext .ts,.tsx",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@aws-sdk/client-cloudfront": "3.430.0",
    "@aws-sdk/client-s3": "3.430.0",
    "recursive-readdir": "2.2.3"
  },
  "devDependencies": {
    "@cowcoders/eslint-config": "2.0.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.34.0",
    "eslint-plugin-github": "4.6.0",
    "eslint-plugin-jest": "27.2.1",
    "graceful-fs": "4.2.10",
    "jest": "29.4.2",
    "jest-circus": "29.4.2",
    "js-yaml": "4.1.0",
    "mime": "3.0.0",
    "shx": "0.3.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "=18.14.0",
    "npm": "=9.3.1"
  },
  "engineStrict": true
}
