{
  "name": "aws-s3-js-action",
  "version": "0.0.3",
  "private": true,
  "description": "Synchronize S3 bucket files",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint --max-warnings 0 . --ext .ts,.tsx",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "raulanatol",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "s3": "^4.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@typescript-eslint/parser": "^3.6.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.5.0",
    "eslint-plugin-github": "^4.1.0",
    "eslint-plugin-jest": "^23.18.0",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
